pico-8 cartridge // http://www.pico-8.com
version 32
__lua__
--actor

actor = {}	--all actors

-- make an actor
-- and add to global collection
-- x, y means center of the actor

function make_actor(k, x, y)
	a={
		--k is what number sprite on the sprite sheet we use
		k = k,
		-- the x and y coordinates
		x = x,
		y = y,
		-- the x and y velocity
		dx = 0,
		dy = 0,
		-- how manyu frames of animation this sprite has
		frame = 0,
		t = 0,
		friction = 0.15,
		bounce = 0.3,
		frames = 2,
		
		-- half-width and half-height
		-- slightly less than 0.5
		-- so that will fit through
		-- 1-wide holes.
		
		w = 0.4,
		h = 0.4,
	}
	add(actor,a)
	return a
end

function _draw()
	cls()		-- clear the screen
	map()		-- draw the map
	foreach(actor,draw_actor)	-- call the function "draw_actor" on each actor
end
